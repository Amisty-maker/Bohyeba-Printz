<script>
  // Price conversion rate (mock)
  const cediToDollarRate = 0.17; // example conversion

  // 20 categories with prices in dollars
  const categories = [
    { name: "Logo Design", priceUSD: 150 },
    { name: "Brand Identity", priceUSD: 300 },
    { name: "Flyers & Posters", priceUSD: 100 },
    { name: "Business Cards", priceUSD: 75 },
    { name: "Packaging", priceUSD: 250 },
    { name: "Website Banners", priceUSD: 120 },
    { name: "Social Media Graphics", priceUSD: 90 },
    { name: "Custom Illustrations", priceUSD: 200 },
    { name: "Typography Design", priceUSD: 110 },
    { name: "Brochures", priceUSD: 130 },
    { name: "Advertising Design", priceUSD: 180 },
    { name: "Event Invitations", priceUSD: 85 },
    { name: "Book Covers", priceUSD: 220 },
    { name: "Postcards", priceUSD: 65 },
    { name: "Menus", priceUSD: 140 },
    { name: "Catalogs", priceUSD: 160 },
    { name: "Signage", priceUSD: 170 },
    { name: "Infographics", priceUSD: 190 },
    { name: "T-shirt Design", priceUSD: 130 },
    { name: "Digital Ads", priceUSD: 100 },
  ];

  const galleryGrid = document.querySelector('.gallery-grid');

  categories.forEach(cat => {
    const priceGHS = (cat.priceUSD / cediToDollarRate).toFixed(0);
    const item = document.createElement('div');
    item.className = 'gallery-item';
    item.tabIndex = 0;
    item.setAttribute('role', 'listitem');
    item.innerHTML = `
      <div class="gallery-img" aria-label="${cat.name} image placeholder">Image</div>
      <div class="category-name">${cat.name}</div>
      <div class="price">Price: $${cat.priceUSD} / â‚µ${priceGHS}</div>
    `;
    galleryGrid.appendChild(item);
  });
</script>
</body>
</html>
</content>
